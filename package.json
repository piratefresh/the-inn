{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:backend": "pnpm workspace backend pnpm dev",
    "dev:web": "pnpm workspace web pnpm dev",
    "dev:storybook": "pnpm workspace docs pnpm storybook",
    "backend:prisma:generate": "cd apps/backend && npx prisma generate",
    "backend:prisma:migrate": "cd apps/backend && npx prisma migrate dev",
    "gen:web": "pnpm workspace web pnpm gen"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.22.0"
}
