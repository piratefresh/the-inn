{
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db:generate", "^db:migration:deploy"],
      "outputs": ["dist/**", ".next/**"]
    },
    "db:generate": {

      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migration:deploy": {
      "cache": false
    },
    "db:migrate:deploy": {
      "cache": false
    }
  }
}
