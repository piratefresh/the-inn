{
  "name": "backend",
  "version": "0.100.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "build:swc:2": "rm -rf dist && prisma migrate deploy && swc src -d dist --source-maps --copy-files",
    "build:tsc": "rm -rf dist && prisma migrate deploy && tsc && tsc-alias -p tsconfig.json",
    "build:swc": "swc . -d dist",
    "build": "rm -rf dist && prisma migrate deploy && tsup index.ts --format cjs",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "db:push": "prisma db push --skip-generate",
    "db:generate": "prisma migrate dev --create-only",
    "db:migration:run": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "studio": "prisma studio"
  },
